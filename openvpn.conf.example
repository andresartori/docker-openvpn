server 192.168.255.0 255.255.255.0
verb 3
key /etc/openvpn/certs/server.key
ca /etc/openvpn/certs/ca.crt
cert /etc/openvpn/certs/server.crt
dh /etc/openvpn/certs/dh.pem
tls-auth /etc/openvpn/certs/ta.key
key-direction 0
keepalive 10 60
persist-key
persist-tun
proto udp
port 1194
dev tun0
user nobody
group nogroup
comp-lzo no
